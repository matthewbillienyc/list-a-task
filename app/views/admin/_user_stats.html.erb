<legend>User Statstics</legend>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Most active lists</th>
      <th># Lists</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @user_with[:most_active_lists].username %></td>
      <td><%= @user_with[:most_active_lists].lists.length %></td>
    </tr>
  </tbody>
  <thead>
    <tr>
      <th>Most total lists</th>
      <th># Lists</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @user_with[:most_total_lists].username %></td>
      <td><%= @user_with[:most_total_lists].lists.with_deleted.length %></td>
    </tr>
  </tbody>
  <thead>
    <tr>
      <th>Most active tasks</th>
      <th># Tasks</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @user_with[:most_active_tasks].username %></td>
      <td><%= @user_with[:most_active_tasks].tasks.length %></td>
    </tr>
  </tbody>
</table>
