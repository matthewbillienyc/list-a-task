<legend>List Results</legend>
<table class="table table-striped table-hover list-table">
  <thead>
    <tr>
      <th colspan="3">
        <label class="select-label">Filter by user: <select id="list-filter">
          <option value="all" selected>all</option>
          <% User.non_admin.each do |user| %>
            <option value="<%= user.username %>"><%= user.username %></option>
          <% end %>
        </select></label>
        <label> Include deleted: <input type="checkbox" class="deleted list" value="true"></label>
      </th>
    </tr>
    <tr>
      <th>List</th>
      <th>User</th>
      <th class="right-align">Deleted</th>
    </tr>
  </thead>
  <tbody>
    <% lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= list.user.username %></td>
        <td class="right-align">
          <% unless list.deleted_at.nil? %>
            â˜“
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
